<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.LieuBean" table="Lieu">
		<id column="idLieu" name="idLieu" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<property column="nomLieu" name="nomLieu" type="java.lang.String" />
		<property column="commentaireLieu" name="commentaireLieu" type="java.lang.String" />
	</class>
</hibernate-mapping>