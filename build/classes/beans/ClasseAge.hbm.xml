<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.ClasseAgeBean" table="ClasseAge">
		<id column="idClasseAge" name="idClasseAge" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<property column="ageMin" name="ageMin" type="java.lang.Integer" />
		<property column="ageMax" name="ageMax" type="java.lang.Integer" />
	</class>
</hibernate-mapping>