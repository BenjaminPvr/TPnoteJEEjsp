<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.ActiviteBean" table="Activite">
		<id column="idActivite" name="idActivite" type="java.lang.Long">
			<generator class="increment" />
		</id>
		<property column="nomActivite" name="nomActivite" type="java.lang.String" />
	</class>
</hibernate-mapping>